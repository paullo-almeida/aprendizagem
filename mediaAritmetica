#include <stdio.h>

int main() {

 float num1, num2, num3, num4, num5, média;
 printf("Digite o valor do primeiro número: \n");
 scanf("%f", &num1);
 printf("Digite o valor do segundo número: \n");
 scanf("%f", &num2);
 printf("Digite o valor do terceiro número: \n");
 scanf("%f", &num3);
 printf("Digite o valor do quarto número: \n");
 scanf("%f", &num4);
 printf("Digite o valor do quinto número: \n");
 scanf("%f", &num5);
 média=((num1+num2+num3+num4+num5)/5);
 printf("A média dos números digitados é %0.2f.",média);

 return 0;
}
